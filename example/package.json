{
  "name": "react-native-bootsplash-example",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean-modules": "rm -rf ./node_modules/react-native-bootsplash/{example,node_modules}",
    "clean": "rm -rf ./node_modules ./ios/Pods",
    "preinstall": "cd .. && yarn build && cd example",
    "postinstall": "yarn clean-modules && pod-install",
    "start": "react-native start",
    "web": "webpack serve",
    "reinstall": "yarn clean && yarn install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-bootsplash": "file:../",
    "react-native-web": "0.19.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.72.9",
    "@types/react": "^18.2.18",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.5.3",
    "metro-react-native-babel-preset": "0.76.7",
    "pod-install": "^0.1.38",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
